{
  "name": "koa-vue",
  "version": "1.0.0",
  "description": "Node.js, Koa.js, MongoDB and Vue.js stack",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev:server": "nodemon index.js",
